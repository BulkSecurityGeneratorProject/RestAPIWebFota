		<addPrimaryKey columnNames="hub_receive_time, device_address"
            constraintName="pk_patient_vest_device_raw_logs"
            tableName="PATIENT_VEST_DEVICE_RAW_LOGS"/>
		<dropDefaultValue tableName="PATIENT_VEST_DEVICE_RAW_LOGS"
			columnName="hub_receive_time" columnDataType="datetime" />
	</changeSet>
</databaseChangeLog>

		