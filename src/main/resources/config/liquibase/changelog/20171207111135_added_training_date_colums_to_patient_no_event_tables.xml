<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

	<property name="now" value="now()" dbms="mysql,h2" />
	<property name="now" value="current_timestamp" dbms="postgresql" />
	<property name="now" value="sysdate" dbms="oracle" />

	<property name="autoIncrement" value="true" dbms="mysql,h2,postgresql" />
	<property name="autoIncrement" value="false" dbms="oracle" />
	<!-- Added existing_first_transmission_date, modified_first_transmission_date to PATIENT_NO_EVENT and PATIENT_NO_EVENT_MONARCH table. -->
	<changeSet author="jhipster" id="20171207111135">
		<addColumn tableName="PATIENT_NO_EVENT">
        	<column name="first_transmission_date_before_update" type="date" />
        	<column name="date_first_transmission_date_updated" type="date" />
        	<column name="first_trans_date_type" type="varchar(255)"/>
        </addColumn>
        	<addColumn tableName="PATIENT_NO_EVENT_MONARCH">
        	<column name="first_transmission_date_before_update" type="date" />
        	<column name="date_first_transmission_date_updated" type="date" />
        	<column name="first_trans_date_type" type="varchar(255)"/>
        </addColumn>
	</changeSet>
</databaseChangeLog>
