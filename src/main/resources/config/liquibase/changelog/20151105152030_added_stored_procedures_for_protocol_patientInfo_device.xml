<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <property name="now" value="now()" dbms="mysql,h2"/>
    <property name="now" value="current_timestamp" dbms="postgresql"/>
    <property name="now" value="sysdate" dbms="oracle"/>

    <property name="autoIncrement" value="true" dbms="mysql,h2,postgresql"/>
    <property name="autoIncrement" value="false" dbms="oracle"/>
    <!--
        Added the entity UserExtension.
    -->
    <changeSet id="20151105152030" author="jhipster">
    	                                       
          <createProcedure path="./../../sql/manage_patient_device.sql"
          procedureName="manage_patient_device" relativeToChangelogFile="true"></createProcedure>
          
          <createProcedure path="./../../sql/manage_patient_user.sql"
          procedureName="manage_patient_user" relativeToChangelogFile="true"></createProcedure>
          
          <createProcedure path="./../../sql/create_patient_protocol.sql"
          procedureName="create_patient_protocol" relativeToChangelogFile="true"></createProcedure>
          
          <createTable tableName="JDE_LOAD_TABLE">
            <column name="id" type="bigint" autoIncrement="${autoIncrement}" startWith="3">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="hillrom_id" type="bigint(20)"/>
            <column name="patient_name" type="varchar(255)"/>
            <column name="patient_zip" type="integer"/>
            <column name="patient_dob" type="timestamp"/>
            <column name="patient_email" type="varchar(255)"/>
            <column name="serial_number" type="varchar(255)"/>
            <column name="type" type="varchar(10)"/>
            <column name="treatments_per_day" type="bigint(20)"/>
            <column name="treatment_label" type="varchar(45)"/>
			<column name="min_minutes_per_treatment" type="bigint(20)"/>
			<column name="max_minutes_per_treatment" type="bigint(20)"/>
			<column name="min_frequency" type="bigint(20)"/>
			<column name="max_frequency" type="bigint(20)"/>
			<column name="min_pressure" type="bigint(20)"/>
			<column name="max_pressure" type="bigint(20)"/>
			<column name="protocol_key" type="varchar(45)"/>
			<column name="equipment_dt" type="timestamp"/>
			<column name="processed" type="integer"/>
			<column name="processed_dt" type="timestamp"/>
         </createTable>
    </changeSet>
</databaseChangeLog>